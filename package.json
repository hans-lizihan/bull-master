{
  "name": "bull-master",
  "description": "Bull queue UI for inspecting and manipulating jobs",
  "version": "1.0.5",
  "keywords": [
    "bull",
    "redis",
    "queue",
    "monitoring"
  ],
  "author": "Hans Li <lizihan0328@gmail.com>",
  "contributors": [],
  "main": "server/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hans-lizihan/bull-master.git"
  },
  "bugs": {
    "url": "https://github.com/hans-lizihan/bull-master/issues"
  },
  "files": [
    "server/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "lint": "NODE_ENV=development eslint client/**/*.{js,jsx} server/**/*.js",
    "build": "yarn build:clean && yarn build:ui",
    "build:publish": "yarn build",
    "build:ui": "NODE_ENV=production webpack",
    "build:clean": "rm -rf ./static ./dist",
    "dev:server": "nodemon example.js",
    "dev:client": "NODE_ENV=development webpack serve --port 4888",
    "test": "NODE_ENV=test jest --coverage --collectCoverageFrom=**/*.{js,jsx}",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "express": "4.17.2",
    "redis-info": "^3.1.0"
  },
  "optionalDependencies": {
    "bull": "^4.2.0",
    "bullmq": "^1.64.0"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/eslint-parser": "7.16.5",
    "@babel/plugin-proposal-optional-chaining": "7.16.7",
    "@commitlint/cli": "16.0.3",
    "@commitlint/core": "16.0.0",
    "@hot-loader/react-dom": "17.0.2",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "axios": "0.25.0",
    "babel-loader": "8.2.3",
    "babel-preset-react-app": "10.0.1",
    "clsx": "1.1.1",
    "commitlint-config-gitmoji": "2.2.5",
    "css-loader": "6.5.1",
    "date-fns": "2.28.0",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "8.7.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "lint-staged": "12.2.1",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "prettier-eslint": "13.0.0",
    "pretty-bytes": "5.6.0",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-loader": "4.13.0",
    "react-request-hook": "2.1.1",
    "react-router": "6.2.1",
    "react-router-dom": "6.2.1",
    "recharts": "2.1.8",
    "style-loader": "3.3.1",
    "supertest": "6.2.2",
    "webpack": "5.66.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.7.3"
  }
}
