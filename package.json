{
  "name": "bull-master",
  "description": "Bull queue UI for inspecting and manipulating jobs",
  "version": "1.0.4",
  "keywords": [
    "bull",
    "redis",
    "queue",
    "monitoring"
  ],
  "author": "Hans Li <lizihan0328@gmail.com>",
  "contributors": [],
  "main": "server/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hans-lizihan/bull-master.git"
  },
  "bugs": {
    "url": "https://github.com/hans-lizihan/bull-master/issues"
  },
  "files": [
    "server/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "lint": "eslint client/**/*.{js,jsx} server/**/*.js && commitlint ci",
    "build": "yarn build:clean && yarn build:ui",
    "build:publish": "yarn build",
    "build:ui": "NODE_ENV=production webpack --hide-modules",
    "build:clean": "rm -rf ./static ./dist",
    "dev:server": "nodemon example.js",
    "dev:client": "NODE_ENV=development webpack-dev-server --hot --client-log-level warning --progress --port 4888",
    "test": "NODE_ENV=test jest --coverage --collectCoverageFrom=**/*.{js,jsx}"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "yarn test"
    }
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "4.17.1",
    "redis-info": "^3.0.7"
  },
  "optionalDependencies": {
    "bull": "^3.13.0",
    "bullmq": "^1.8.3"
  },
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@babel/plugin-proposal-optional-chaining": "7.10.4",
    "@commitlint/cli": "9.1.1",
    "@commitlint/core": "9.1.1",
    "@hot-loader/react-dom": "16.13.0",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "axios": "0.19.2",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-preset-react-app": "9.1.2",
    "clsx": "1.1.1",
    "commitlint-config-gitmoji": "1.0.1",
    "css-loader": "3.6.0",
    "date-fns": "2.14.0",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.5.0",
    "eslint": "7.4.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "4.0.7",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "lint-staged": "10.2.11",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "prettier-eslint": "11.0.0",
    "pretty-bytes": "5.3.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hot-loader": "4.12.21",
    "react-request-hook": "2.1.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "recharts": "2.0.0-beta.6",
    "style-loader": "1.2.1",
    "supertest": "4.0.2",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  }
}
